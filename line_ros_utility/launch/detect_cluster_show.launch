<launch>
  <arg name="trajectory" default ="1" />
  <arg name="write_path" />
  <node
    pkg="rqt_reconfigure"
    type="rqt_reconfigure"
    name="rqt_reconfigure"
 ></node>
  <node
    pkg="line_ros_utility"
    type="scenenet_to_line_tools"
    name="scenenet_to_line_tools"
    output="screen"
 ></node>
  <node
    pkg="line_ros_utility"
    type="detect_cluster_show"
    name="detect_cluster_show"
    args="$(arg trajectory) $(arg write_path)"
    output="screen"
 ></node>
</launch>
